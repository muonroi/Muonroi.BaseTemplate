{
  "DatabaseConfigs": {
    "DbType": "Sqlite",
    "ConnectionStrings": {
      "SqliteConnectionString": "Data Source=app.db"
    }
  },
  "CacheConfigs": {
    "CacheType": "MultiLevel"
  },
  "RedisConfigs": {
    "Enable": false,
    "Host": "localhost",
    "Port": "6379",
    "Password": "",
    "Expire": 30,
    "KeyPrefix": "app",
    "AllMethodsEnableCache": false
  },
  "FeatureFlags": {
    "UseGrpc": false,
    "UseServiceDiscovery": false,
    "UseMessageBus": false,
    "UseBackgroundJobs": false,
    "UseEnsureCreatedFallback": true
  },
  "TokenConfigs": {
    "Issuer": "https://example.com",
    "Audience": "https://example.com",
    "SigningKeys": "",
    "UseRsa": false,
    "ExpiryMinutes": 30,
    "EnableCookieAuth": true,
    "CookieName": "AuthToken",
    "CookieSameSite": "Lax",
    "SymmetricSecretKey": "basetemplate-dev-secret-key-32-chars-2026"
  },
  "MultiTenantConfigs": {
    "Enabled": false
  },
  "TenantConfigs": {
    "DefaultTenant": "tenant1"
  },
  "PaginationConfigs": {
    "DefaultPageIndex": 1,
    "DefaultPageSize": 10,
    "MaxPageSize": 10
  },
  "ResourceSetting": {
    "ResourceName": "Resources.ErrorMessages",
    "lang": "vi-VN"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    }
  },
  "MAllowDomains": "https://localhost:52182,http://localhost:4200",
  "GrpcServices": {
    "Services": {
      "SampleService": {
        "Uri": "https://localhost:5001"
      }
    }
  },
  "ConsulConfigs": {
    "ServiceName": "MyService",
    "ConsulAddress": "http://localhost:8500",
    "ServiceAddress": "http://localhost",
    "ServicePort": 5000
  },
  "MessageBusConfigs": {
    "BusType": "Kafka",
    "Kafka": {
      "Host": "localhost:9092",
      "Topic": "sample-topic",
      "GroupId": "sample-group"
    }
  },
  "SecretKey": "",
  "EnableEncryption": false,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "LicenseConfigs": {
    "Mode": "Offline",
    "LicenseFilePath": null,
    "PublicKeyPath": null,
    "PolicyFilePath": null,
    "FallbackToOnlineActivation": false,
    "ProjectSeed": "MUONROI_BASE_SECURE_SEED_20260220",
    "EnableChain": false,
    "ChainStorage": "File",
    "ChainFilePath": "logs/license-chain.log",
    "FailMode": "Soft",
    "EnforceOnDatabase": false,
    "EnforceOnMiddleware": false,
    "Online": {
      "Endpoint": "http://localhost:6000",
      "ChainSubmissionEndpoint": "/api/v1/chain/submit",
      "TimeoutSeconds": 10,
      "RefreshMinutes": 60
    }
  }
}
